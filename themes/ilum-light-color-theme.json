{
	"$schema": "vscode://schemas/color-theme",
	"name": "Ilum Light",
	"tokenColors": [
		/**
		 * Based on:
		 * https://github.com/tonsky/vscode-theme-alabaster/blob/master/theme/alabaster-color-theme.json
		 */
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Strings",
			"scope": ["string", "string.regexp", "constant.other.symbol"],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": ["constant.character.escape"],
			"settings": {
				"foreground": "#8D8D8D"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": ["constant"],
			"settings": {
				"foreground": "#9240c2"
			}
		},
		{
			"name": "Global definitions",
			"scope": ["entity.name"],
			"settings": {
				"foreground": "#325CC0"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation"],
			"settings": {
				"foreground": "#8D8D8D"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid"],
			"settings": {
				"foreground": "#600"
			}
		},
		{
			"name": "JSDoc",
			"scope": ["storage.type.class.jsdoc", "variable.other.jsdoc"],
			"settings": {
				"foreground": "#3B3B3B"
			}
		},
		{
			"name": "Properties",
			"scope": ["entity.name.tag", "meta.objectliteral", "support.type.property-name"],
			"settings": {
				"foreground": "#3B3B3B"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": ["meta.diff.header.from-file"],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": ["meta.diff.header.to-file"],
			"settings": {
				"foreground": "#434343"
			}
		}
	],
